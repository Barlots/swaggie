{
  "name": "swaggie",
  "version": "0.1.9",
  "description": "Generate ES6 or TypeScript service integration code from an OpenAPI spec",
  "author": {
    "name": "Piotr Dabrowski",
    "url": "https://github.com/yhnavein"
  },
  "license": "MIT",
  "homepage": "https://github.com/yhnavein/swaggie",
  "repository": {
    "type": "git",
    "url": "https://github.com/yhnavein/swaggie.git"
  },
  "bugs": {
    "url": "https://github.com/yhnavein/swaggie/issues"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "dist/index.js",
  "bin": {
    "swaggie": "dist/cli.js"
  },
  "scripts": {
    "prebuild": "npm run init",
    "build": "tsc",
    "test": "jest --ci",
    "test:watch": "jest --watch",
    "predev": "npm run init",
    "dev": "tsc -w",
    "init": "npm run clean",
    "clean": "shx rm -rf ./dist"
  },
  "files": [
    "dist",
    "templates"
  ],
  "keywords": [
    "swagger",
    "swagger 2.0",
    "openapi",
    "rest",
    "service",
    "typescript",
    "codegen"
  ],
  "dependencies": {
    "chalk": "2.4.2",
    "commander": "2.20.0",
    "ejs": "2.6.2",
    "got": "9.6.0",
    "js-yaml": "3.13.1",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "@types/ejs": "2.6.3",
    "@types/got": "9.6.0",
    "@types/jest": "24.0.15",
    "@types/js-yaml": "3.12.1",
    "@types/lodash": "4.14.135",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "12.0.12",
    "@types/prettier": "1.16.4",
    "jest": "24.8.0",
    "shx": "^0.3.2",
    "ts-jest": "24.0.2",
    "tslint": "5.18.0",
    "typescript": "3.5.2"
  },
  "jest": {
    "testRegex": "\\.(test|spec)\\.tsx?$",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
